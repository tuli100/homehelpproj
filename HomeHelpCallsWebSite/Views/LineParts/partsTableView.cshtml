@model  IEnumerable<HomeHelpCallsWebSite.Models.PartViewModel>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td align="right">

                @Html.ActionLink(item.PART_CODE_NAME + " - " + item.UNIT_NAME, null, null, new { id = Model.First().doc_nbr, part = item.PART_CODE_NAME }, null)
            </td>
            <td class="tr-for-media"></td>
        </tr>

    }

</table>
